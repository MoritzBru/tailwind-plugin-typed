{
  "name": "tailwind-plugin-typed",
  "version": "0.2.0",
  "type": "module",
  "repository": "https://github.com/MoritzBru/tailwind-plugin-typed",
  "author": {
    "name": "Moritz BrunnengrÃ¤ber",
    "email": "mail@moritz-brunnengraeber.de"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:serve": "vite serve --config dev/vite.config.ts",
    "dev:plugin": "tsup src/index.ts --format esm --watch",
    "build": "tsup src/index.ts --clean --format esm,cjs --minify --dts",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.12.12",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "globals": "^15.2.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.9.0",
    "vite": "^5.2.0"
  }
}
