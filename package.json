{
  "name": "tailwind-plugin-typed",
  "description": "A plugin for Tailwind CSS to generate text typing animations.",
  "version": "0.5.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoritzBru/tailwind-plugin-typed.git"
  },
  "author": {
    "name": "Moritz BrunnengrÃ¤ber",
    "email": "mail@moritz-brunnengraeber.de"
  },
  "license": "MIT",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "plugin",
    "css",
    "animation",
    "typing",
    "typewriter"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:serve": "vite serve --config dev/vite.config.ts",
    "dev:plugin": "tsup src/index.ts --format esm --watch",
    "build": "tsup src/index.ts --clean --format esm,cjs --dts",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/node": "^20.12.12",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.3.0",
    "globals": "^15.3.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.14",
    "vite": "^5.2.11"
  }
}
